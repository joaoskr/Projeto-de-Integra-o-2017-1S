<!DOCTYPE html>
<html lang="pt-br">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="Projeto integrador 2017 1º semestre - Universidade do Sagrado coração">
	<meta name="author" content="Rodrigo Gonçalves">
	<link rel="icon" href="/Models/reader.png">

	<title>@ViewBag.Title</title>

	<!-- Fonte dos textos -->
	<link href="https://fonts.googleapis.com/css?family=El+Messiri" rel="stylesheet">
	<link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">

	<!-- Bootstrap CSS -->
	<link href="/Content/bootstrap.min.css" rel="stylesheet">
	<!-- Stylesheet principal -->
	<link href="/Content/master.css" rel="stylesheet">

</head>

<body>
	<!-- Barra de Navegação -->
	<nav class="navbar navbar-inverse navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
				<a class="navbar-brand" href="/Home/Index">Projeto integrador</a>
			</div>
			<div id="navbar" class="collapse navbar-collapse">
				<ul class="nav navbar-nav">
					<li class="@ViewBag.Model1"><a href="/Home/Index">Home</a></li>
					<li class="@ViewBag.Model2"><a href="/Home/Circuitos">Circuitos</a></li>
					<li class="@ViewBag.Model3"><a href="/Home/Estrutura">Estrutura I</a></li>
					<li class="@ViewBag.Model4"><a href="/Home/Sistemas">Sistemas</a></li>
				</ul>
			</div>
		</div>
	</nav>

	<!-- Conteudo a ser carregado -->
	<div class="container">
		<div class="starter-template">
			@RenderBody()
		</div>
	</div>

	<!-- Rodapé -->
	<footer class="rodape">
		<div class="footer" id="footer">
			&copy; Projeto integrador 2017.
			<ul style="list-style-type: none;" class="pull-right social">
				<li> <a href="https://github.com/rodrigogons/Projeto-de-Integra-o-2017-1S" target="_blank"> <i class="fa fa-github"> Código Fonte</i></a></li>
			</ul>
		</div>
	</footer>

	<!-- Bootstrap javaScript principal -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
	<script>
		window.jQuery || document.write('<script src="/Scripts/jquery.min.js"><\/script>')
	</script>
	<script src="/Scripts/bootstrap.min.js"></script>
</body>

</html>
